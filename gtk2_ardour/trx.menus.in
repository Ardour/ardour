  <menubar name='Main' action='MainMenu'>
    <menu name='Session' action='Session'>
      <menuitem action='New'/>
      <menuitem action='Open'/>
      <menuitem action='Recent'/>
      <menuitem action='Close'/>
      <separator/>
      <menuitem action='Save'/>
