#: opts.cc:58
msgid "  [SESSION_NAME]              Name of session to load\n"
msgstr "  [SESSION_NAME]              要载入的会话的名称\n"

#: opts.cc:59
msgid "  -v, --version               Show version information\n"
msgstr "  -v, --version               显示版本信息\n"

#: opts.cc:60
msgid "  -h, --help                  Print this message\n"
msgstr "  -h, --help                  打印本信息\n"

