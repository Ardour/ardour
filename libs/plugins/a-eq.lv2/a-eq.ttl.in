        lv2:index 11 ;
        lv2:name "Frequency 4" ;
        lv2:symbol "freq4" ;
        lv2:default 6000.000000 ;
        lv2:minimum 20.000000 ;
        lv2:maximum 20000.000000 ;
        unit:unit unit:hz0 ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#logarithmic> ;
