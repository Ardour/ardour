    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 24 ;
        lv2:symbol "in_1" ;
        lv2:name "Audio Input 1" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 25 ;
        lv2:symbol "out_1" ;
        lv2:name "Audio Output 1" ;
    ] ;

    rdfs:comment """
A basic 4 band EQ.
""" ;

    doap:name "a-EQ" ;
    doap:license "GPL v2+" ;
    doap:maintainer <http://ardour.org/credits.html> ;

    lv2:microVersion 0 ;
    lv2:minorVersion 1 .
